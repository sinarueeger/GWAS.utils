options(htmltools.dir.version = FALSE)
library(tidyverse)
library(knitr)
library(kableExtra) # for nice tables
# TABLE WITH ONLY BINARY TRAIT
myTable = data.frame("G"=c("...", "??"), "GxG"=c("-","??"))
rownames(myTable) =c("binary", "continuous")
myTable %>%
mutate(
TYPE = row.names(.),
G = cell_spec(G, "html", color = ifelse(G == "??", "white", " #BAAFAE"), bold = ifelse(G == "??", FALSE, TRUE)),
GxG = cell_spec(GxG, "html", color="white")
) %>%
select(TYPE, G, GxG) %>%
kable("html", escape = F, col.names=c("", "G", ""),  align="c") %>%
kable_styling(font_size=25) %>%
column_spec(2:3, width = "30em") %>%
row_spec(2, color = "white")
# TABLE WITH EXTENSION TO CONTINUOUS TRAIT
myTable = data.frame("G"=c("...", "??"), "GxG"=c("-","??"))
rownames(myTable) =c("binary", "continuous")
myTable %>%
mutate(
TYPE = row.names(.),
G = cell_spec(G, "html", color = ifelse(G == "??", " #BAAFAE", "black"), bold = ifelse(G == "??", TRUE,FALSE)),
GxG = cell_spec(GxG, "html", color="white")
) %>%
select(TYPE, G, GxG) %>%
kable("html", escape = F, col.names=c("", "G", ""),  align="c") %>%
kable_styling(font_size=25) %>%
column_spec(2:3, width = "30em")
# TABLE WITH EXTENSION TO GxG FOR CONTINUOUS TRAIT
myTable = data.frame("G"=c("...", "??"), "GxG"=c("-","??"))
rownames(myTable) =c("binary", "continuous")
myTable %>%
mutate(
TYPE = row.names(.),
G = cell_spec(G, "html", color = "black"),
GxG = cell_spec(GxG, "html", color=ifelse(GxG=="??", " #BAAFAE", "grey"), bold = ifelse(GxG == "??", TRUE,FALSE))
) %>%
select(TYPE, G, GxG) %>%
kable("html", escape = F, col.names=c("", "G", "GxG"),  align="c") %>%
kable_styling(font_size=25) %>%
column_spec(2:3, width = "30em")
